CREATE TYPE Maybe AS VARIANT{ Some(Integer), None()};

CREATE TABLE tab (may Maybe, val Integer);

INSERT INTO tab (may, val) VALUES (Some(1), 1);
INSERT INTO tab (may, val) VALUES (None(), 2);
INSERT INTO tab (may, val) VALUES (Some(2), 3);
INSERT INTO tab (may, val) VALUES (Some(2), 4);

SELECT may FROM tab WHERE val = 1;
SELECT may FROM tab WHERE may: Some(_);
SELECT may FROM tab WHERE may: Some(_);
SELECT may FROM tab WHERE may: Some(_), val = 1;

CREATE TYPE MaybeMaybe AS VARIANT{ Some(Maybe, Integer), None()};

CREATE TABLE tab2 (may MaybeMaybe, val Integer);

INSERT INTO tab2 (may, val) VALUES (Some(Some(1), 1), 1);
INSERT INTO tab2 (may, val) VALUES (None(), 2);
INSERT INTO tab2 (may, val) VALUES (Some(Some(2), 3), 3);
INSERT INTO tab2 (may, val) VALUES (Some(None(), 4), 4);

SELECT may FROM tab2 WHERE may: Some(_, _);
SELECT may FROM tab2 WHERE may: Some(Some(_), _);
SELECT may FROM tab2 WHERE may: Some(_, _), val = 1;



